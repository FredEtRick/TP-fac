package TP3ex1;

public class TestCPile
{
	public static void main(String[] args)
	{
		IPile<String> pileS = new CPile<string>();
		for (int i=0 ; i < 10 ; i++)
		{
			pileS.empile(new Integer(i).toString());
		}
		pileS.depile();
		pileS.depile();
		System.out.println("La pile : " + pileS);
		System.out.println("Nombre d'éléments dans la pile : ") + pileS.nbElements();
		System.out.println("Sommet de pile : " + pileS.sommet());
	}
}
